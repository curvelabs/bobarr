{
  "$schema": "https://aka.ms/devcontainer.schema.json",
  "name": "Bobarr Development",
  "dockerComposeFile": [
    "docker-compose.yml",
    "docker-compose.dev.yml",
    ".devcontainer/docker-compose.extend.yml"
  ],
  "service": "workspace",
  "workspaceFolder": "/workspace",
  "customizations": {
    "codespaces": {
      "repositories": {
        "*/*": {
          "permissions": "write-all"
        }
      }
    }
  }
}
